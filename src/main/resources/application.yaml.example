spring:
  datasource:
    url: jdbc:h2:file:./db/mydb;MODE=MySQL;DB_CLOSE_DELAY=-1
    driver-class-name: org.h2.Driver
    username: sa
    password:
  h2:
    console:
      enabled: true
      path: /h2-console
  jpa:
    hibernate:
      ddl-auto: update
    database-platform: org.hibernate.dialect.H2Dialect

isthereanydeal:
  api:
    key: ${ISTHEREANYDEAL_API_KEY}
  deal:
    url: ${ISTHEREANYDEAL_DEAL_URL}
  game:
    search:
      url: ${ISTHEREANYDEAL_GAME_SEARCH_URL}

server:
  port: ${API_PORT}